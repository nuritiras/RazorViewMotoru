@{
    string[] kurslar = { "MVC Kursu", "HTML Kursu", "CSS Kursu", "JavaScript Kursu" };
}
@model string[]

<h2>Kurslar</h2>
<hr />
<ul>
    @foreach (var kurs in kurslar)
    {
        <li>
            @kurs
        </li>
    }
</ul>
<h2>Programlama Dilleri</h2>
<hr />
<ul>
    @foreach (var dil in Model)
    {
        <li>
            @dil
        </li>
    }
</ul>